{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "lib": [
      "es6",
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "removeComments": true,
    "typeRoots": [
      "./node_modules/@types",
      "./src/@types"
    ],
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react",
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@assets": [
        "src/assets"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@contexts": [
        "src/contexts"
      ],
      "@contexts/*": [
        "src/contexts/*"
      ],
      "@interfaces": [
        "src/interfaces"
      ],
      "@interfaces/*": [
        "src/interfaces/*"
      ],
      "@components": [
        "src/components"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@pages": [
        "src/pages"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@services": [
        "src/services"
      ],
      "@services/*": [
        "src/services/*"
      ],
      "@styles": [
        "src/styles"
      ],
      "@styles/*": [
        "src/styles/*"
      ],
      "@utils": [
        "src/utils"
      ],
      "@utils/*": [
        "src/utils/*"
      ]
    }
  }
}